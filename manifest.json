{
    "manifest_version": 3,
    "name": "Custom LeetCode Generator",
    "version": "1.0",
    "description": "Opens a random LeetCode problem based on your preferences.",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "https://leetcode.com/*"
    ]
}